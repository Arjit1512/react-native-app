{
  "expo": {
    "name": "True Hood",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sources/real-logo-hstar.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arjit.truehood",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.arjit.truehood",
      "adaptiveIcon": {
        "foregroundImage": "./assets/sources/real-logo-hstar.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/sources/real-logo-hstar.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/sources/real-logo-hstar.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "fonts": {
      "Inconsolata-Regular": "./assets/fonts/Inconsolata_400Regular.ttf",
      "Inconsolata-Bold": "./assets/fonts/Inconsolata_700Bold.ttf",
      "SpaceMono-Regular": "./assets/fonts/SpaceMono-Regular.ttf"
    },
    "experiments": {
      "typedRoutes": true
    },
    "typeRoots": [
      "../node_modules/@types",
      "../@types"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c91c5b17-8c35-4294-a5dc-d331829c40a3"
      }
    }
  }
}
